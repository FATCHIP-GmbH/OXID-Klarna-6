{
  "name": "topconcepts/oxid-klarna-6",
  "description": "Klarna module for OXID eShop version 6.X",
  "type": "oxideshop-module",
  "extra": {
    "oxideshop": {
      "blacklist-filter": [
        "Component/**/*",
        "Controller/**/*",
        "Core/**/*",
        "Documentation/**/*",
        "Model/**/*",
        "Tests/**/*",
        "CHANGELOG.md",
        "composer.json",
        "README.md"
      ],
      "target-directory": "tc/tcklarna"
    }
  },
  "require": {
    "php": ">=5.6",
    "rmccue/requests": ">=1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "phpunit/phpunit-selenium": ">=1.2"
  },
  "autoload": {
    "psr-4": {
      "TopConcepts\\Klarna\\": "./"
    }
  }
}
