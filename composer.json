{
  "name": "fatchip-gmbh/oxid-klarna-6",
  "description": "Klarna module for OXID eShop version 6.X",
  "type": "oxideshop-module",
  "license": "Apache-2.0",
  "extra": {
    "oxideshop": {
      "blacklist-filter": [
        "documentation/**/*.*"
      ],
      "target-directory": "tc/tcklarna"
    }
  },
  "require": {
    "php": ">=5.6",
    "rmccue/requests": "^1.0",
    "viison/address-splitter": "^0.3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "phpunit/phpunit-selenium": ">=1.2"
  },
  "replace": {
    "topconcepts/oxid-klarna-6": "*"
  },
  "autoload": {
    "psr-4": {
      "TopConcepts\\Klarna\\": "../../../source/modules/tc/tcklarna"
    }
  }
}